{
  "name": "automate-my-stuff-frontend",
  "version": "0.2.0",
  "private": true,
  "project-type": "react",
  "author": "Earnest Angel",
  "license": "MIT",
  "scripts": {
    "--------start-commands--------": "",
    "start": "next start",
    "dev": "next dev",
    "uat": "concurrently \"npm run start\" \"npm run test:functional\" \"npx wait-on http://localhost:3000 && npm run test:e2e\"",
    "--------build-commands--------": "",
    "build": "cd e2e && npm i && cd .. && next build",
    "heroku-postbuild": "cd e2e && npm i && cd .. && next build",
    "--------test-commands--------": "",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "test:functional": "cross-env NODE_ENV=local cucumber-js --publish",
    "test:e2e": "cd e2e && npm i && npm run test-and-report",
    "test:e2e-test": "cd e2e && npm i && npm run test",
    "test:e2e-report": "cd e2e && npm i && npm run report",
    "lint": "next lint",
    "------npm-bump-commands-------": "",
    "bump:patch": "npm version patch --force",
    "bump:minor": "npm version minor --force",
    "bump:major": "npm version major --force"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.3.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@next/font": "13.1.1",
    "concurrently": "^8.2.0",
    "custom-env": "^2.0.2",
    "lottie-react": "^2.3.1",
    "next": "13.1.1",
    "next-auth": "^4.18.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "4.9.4",
    "cross-env": "^7.0.3",
  },
  "devDependencies": {
    "@types/testing-library__jest-dom": "5.14.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "jest": "^29.6.1",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.1",
    "ts-jest": "^29.1.1",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5"
  }
}
